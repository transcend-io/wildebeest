{
  "name": "@transcend-io/wildebeest",
  "version": "2.17.3",
  "description": "Type-safe sequelize with a simplified migration framework",
  "main": "build/src/index.js",
  "homepage": "https://github.com/transcend-io/wildebeest#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/transcend-io/wildebeest.git"
  },
  "author": "Transcend Privacy Inc.",
  "license": "MIT",
  "bugs": "https://github.com/transcend-io/wildebeest/issues",
  "files": [
    "build/**/*",
    "src/**/*",
    "tsconfig.json"
  ],
  "engines": {
    "npm": ">=6.4.1",
    "node": ">=10.15.0"
  },
  "bin": {
    "reorder-migrations": "./build/scripts/reorder_migrations.js"
  },
  "moduleSystem": "typescript",
  "private": false,
  "scripts": {
    "####### Linting #######": "",
    "lint": "eslint . --cache --ext .js,.ts,.tsx,.jsx",
    "lint:fix": "eslint . --cache --fix --ext .js,.ts,.tsx,.jsx",
    "ts:lint": "tslint ./src/**/*.ts",
    "####### Testing #######": "npm run build && npm run test:nobuild",
    "test": "echo no tests",
    "test:nobuild": "NODE_ENV=test ./node_modules/.bin/mocha \"build/**/*.test.{js,ts}\" --reporter spec --timeout 10000",
    "####### Build #######": "",
    "prepare": "npm run build",
    "build": "rimraf build && tsc && copy './generateMixins/{**/*.hbs,**/*.json}' build/generateMixins && copy './src/{**/*.hbs,**/*.json}' build/src && node ./fix_absolute_imports.js",
    "build:watch": "tsc --watch"
  },
  "peerDependencies": {
    "sequelize": "^5.21.6"
  },
  "dependencies": {
    "aws-sdk": "^2.687.0",
    "bluebird": "=3.7.2",
    "change-case": "4.1.1",
    "express": "^4.17.1",
    "handlebars": "^4.7.6",
    "lodash": "^4.17.15",
    "pluralize": "^8.0.0",
    "umzug": "^2.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.4",
    "@types/lodash": "^4.14.154",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.9",
    "@types/pluralize": "0.0.29",
    "@types/umzug": "^2.2.3",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "babel-eslint": "^10.1.0",
    "copy": "^0.3.2",
    "eslint": "^7.1.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.2",
    "mocha": "^7.2.0",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "sequelize": "^5.21.11",
    "tslint": "^6.1.1",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "3.9.3"
  }
}
